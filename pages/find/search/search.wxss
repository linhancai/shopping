/* pages/find/search/search.wxss */
page {
  background: #fff;
  height: 100%;
}
.search-box{
  position:fixed;          
     background: #f3f3f3; 
}
.search-box .icons{
top:46rpx;
left:56rpx;

}
.search-box .closeSearch{
   position: absolute;
  top:5px;
right:15%;
  width: 55rpx;
  height: 49rpx;
  z-index: 44;
  background-position: -54rpx -643rpx;
}
.searchListWran{
  width: 100%;
  height: 85%;
  overflow:auto;
  position: absolute;
  top: 130rpx;
  left: 0;
  background: #fff;
  z-index: 20;
}
.search-input {
  text-align: left;
  color: #333;
  padding-left: 11%;
  margin:0 100rpx 0 30rpx;
}
.searchingBtn{
 position:absolute;
top:32rpx;
right:10rpx;
font-family:PingFangSC-Medium;
color:#F45C43;
font-size:32rpx;
height:70rpx;
width:80rpx;
line-height:70rpx;
}
.wxSearch {
  width: 100%;
  height: 100%;
 padding-top:150rpx;
 box-sizing: border-box;
}

.wxSearchInner {
  background-color: #fff;
}

.wxSearch-section {
  background-color: #fff;
  padding: 5px 10px;
  border: 1px #eee solid;
}

.wxSearch-pancel {
  display: flex;
  flex-direction: row;
}

.wxSearch-input {
  flex: 8;
  background-color: #eee;
  padding: 5px 10px;
}

.wxSearch-button {
  flex: 1;
  border: none !important;
  color: cornflowerblue !important;
}

.wxSearchTitle {
  display: block;
  padding: 2px 5px 0px 10px;
 font-family: PingFangSC-Regular;
font-size: 28rpx;
color: #333333;
line-height:60rpx;

}

.icons {
  position: absolute;
  top: 16px;
  left: 7%;
  width: 20px;
  height: 20px;
  z-index: 444;
}

.wxSearchMindKeyItem {
 padding:10px 40rpx 10rpx 60rpx;
font-family:PingFangSC-Regular;
color:#666666;
font-size:26rpx;
line-height:40rpx;
border-bottom:1rpx solid #eee;
}
.hoverItem{
background: #FFEEE9;
border-radius: 100px;
color: #F45C43;
}


.wxSearchKeyList {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.wxSearchKey{
  padding-left: 30rpx;
}

.wxSearchHistory{
  padding-bottom: 30rpx;
  margin:0 30rpx;
  border-bottom: 1px solid #EEEEEE;
}
.wxSearchHistoryItem {
  padding: 10px 40rpx ;
  font-family: PingFangSC-Regular;
color: #666666;
  font-size: 26rpx;
  display: inline-block;
 background: #EEEEEE;
border-radius: 100px;
  margin: 10rpx;
}

.wxSearchHistoryItemDel {
  width: 22px;
  height: 22px;
}

.wxSearchHistoryEmpty {
  text-align: left;
  height: 25px;
  line-height: 20px;
  padding-left: 10px;
  color: #777;
  font-size: 12px;
}

.wxSearchDeleteAllBtn {
  height: 44rpx;
  width: 44rpx;
  background-position: -54rpx -170rpx;
  float: right;
  margin-top:6rpx;
}


